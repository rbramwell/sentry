{
  "name": "Sentry",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/getsentry/sentry.git"
  },
  "modulePaths": ["<rootDir>/src/sentry/static/sentry"],
  "setupFiles": ["<rootDir>/tests/js/setup.js"],
  "testPathIgnorePatterns": ["<rootDir>/tests/sentry/lang/javascript/"],
  "unmockedModulePathPatterns": [
    "<rootDir>/node_modules/react",
    "<rootDir>/node_modules/reflux"
  ],
  "devDependencies": {
    "@percy-io/react-percy-storybook": "^1.0.2",
    "@storybook/addon-actions": "^3.2.11",
    "@storybook/addon-info": "^3.2.11",
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/react": "^3.2.11",
    "babel-eslint": "7.2.3",
    "babel-jest": "^19.0.0",
    "chai": "3.4.1",
    "enzyme": "2.9.1",
    "eslint": "4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-sentry-react": "1.0.4",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-getsentry": "1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "7.4.0",
    "jest": "^19.0.2",
    "prettier": "1.7.4",
    "sinon": "1.17.2",
    "sinon-chai": "2.8.0",
    "webpack-dev-server": "^2.7.1"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.2"
  }
}
